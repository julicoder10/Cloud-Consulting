<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Project_in_progress</fullName>
    <active>false</active>
    <errorConditionFormula>AND(
NOT(ISNEW()),
NOT(ISCHANGED(State__c))
)</errorConditionFormula>
    <errorMessage>The Proyect is In Progress and no modifications can be done</errorMessage>
</ValidationRule>
